.row.wrapper.border-bottom.white-bg.page-heading
  .col-lg-10
    %h2 Products grid
    %ol.breadcrumb
      %li
        %a{:href => ""} Home
      %li
        %a E-commerce
      %li.active
        %strong Products grid
