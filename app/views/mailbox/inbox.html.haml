                  %i.fa.fa-circle.text-danger
                  Documents
              %li
                %a{:href => "#"}
                  %i.fa.fa-circle.text-primary
                  Social
              %li
                %a{:href => "#"}
                  %i.fa.fa-circle.text-info
                  Advertising
              %li
                %a{:href => "#"}
                  %i.fa.fa-circle.text-warning
                  Clients
            %h5.tag-title Labels
            %ul.tag-list{:style => "padding: 0"}
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Family
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Work
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Home
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Children
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Holidays
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Music
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Photography
              %li
                %a{:href => ""}
                  %i.fa.fa-tag
                  Film
            .clearfix
    .col-lg-9.animated.fadeInRight
      .mail-box-header
        %form.pull-right.mail-search{:action => "#", :method => "get"}
          .input-group
            %input.form-control.input-sm{:name => "search", :placeholder => "Search email", :type => "text"}/
            .input-group-btn
              %button.btn.btn-sm.btn-primary{:type => "submit"}
                Search
        %h2
          Inbox (16)
        .mail-tools.tooltip-demo.m-t-md
          .btn-group.pull-right
            %button.btn.btn-white.btn-sm
              %i.fa.fa-arrow-left
            %button.btn.btn-white.btn-sm
              %i.fa.fa-arrow-right
          %button.btn.btn-white.btn-sm{"data-placement" => "left", "data-toggle" => "tooltip", :title => "Refresh inbox"}
            %i.fa.fa-refresh
            Refresh
          %button.btn.btn-white.btn-sm{"data-placement" => "top", "data-toggle" => "tooltip", :title => "Mark as read"}
            %i.fa.fa-eye
          %button.btn.btn-white.btn-sm{"data-placement" => "top", "data-toggle" => "tooltip", :title => "Mark as important"}
            %i.fa.fa-exclamation
          %button.btn.btn-white.btn-sm{"data-placement" => "top", "data-toggle" => "tooltip", :title => "Move to trash"}
            %i.fa.fa-trash-o
