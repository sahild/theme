.wrapper.wrapper-content
  .row
    .col-lg-3
      .ibox.float-e-margins
        .ibox-content.mailbox-content
          .file-manager
            %a.btn.btn-block.btn-primary.compose-mail{:href => "#{url_for :controller => 'mailbox', :action => 'compose_email'}"} Compose Mail
            .space-25
            %h5 Folders
            %ul.folder-list.m-b-md{:style => "padding: 0"}
              %li
                %a{:href => "#{url_for :controller => 'mailbox', :action => 'inbox'}"}
                  %i.fa.fa-inbox
                  Inbox
                  %span.label.label-warning.pull-right 16
              %li
                %a{:href => "#{url_for :controller => 'mailbox', :action => 'inbox'}"}
                  %i.fa.fa-envelope-o
