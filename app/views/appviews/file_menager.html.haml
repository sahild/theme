                  %i.fa.fa-folder
                  Films
              %li
                %a{:href => ""}
                  %i.fa.fa-folder
                  Books
            %h5.tag-title Tags
            %ul.tag-list{:style => "padding: 0"}
              %li
                %a{:href => ""} Family
              %li
                %a{:href => ""} Work
              %li
                %a{:href => ""} Home
              %li
                %a{:href => ""} Children
              %li
                %a{:href => ""} Holidays
              %li
                %a{:href => ""} Music
              %li
                %a{:href => ""} Photography
              %li
                %a{:href => ""} Film
            .clearfix
