.row.wrapper.border-bottom.white-bg.page-heading
  .col-lg-9
    %h2 File Manager
    %ol.breadcrumb
      %li
        %a{:href => "#{url_for :controller => 'dashboards', :action => 'dashboard_1'}"} Home
      %li
        App Views
      %li.active
        %strong File Manager
.wrapper.wrapper-content
  .row
    .col-lg-3
      .ibox.float-e-margins
        .ibox-content
          .file-manager
            %h5 Show:
            %a.file-control.active{:href => "#"} Ale
            %a.file-control{:href => "#"} Documents
            %a.file-control{:href => "#"} Audio
            %a.file-control{:href => "#"} Images
            .hr-line-dashed
            %button.btn.btn-primary.btn-block Upload Files
            .hr-line-dashed
            %h5 Folders
            %ul.folder-list{:style => "padding: 0"}
              %li
                %a{:href => ""}
                  %i.fa.fa-folder
                  Files
              %li
                %a{:href => ""}
                  %i.fa.fa-folder
                  Pictures
              %li
                %a{:href => ""}
                  %i.fa.fa-folder
